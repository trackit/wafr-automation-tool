  "scripts": {
    "start:webui": "nx serve webui",
    "build:webui": "nx build webui --skipNxCache",
    "test:webui": "nx run-many -t test -p tag:scope:webui tag:scope:webui-libs tag:scope:shared",
    "lint:webui": "nx run-many -t lint -p tag:scope:webui tag:scope:webui-libs tag:scope:shared",
    "build:backend": "nx build backend --skipNxCache",
    "deploy:backend": "nx deploy backend",
    "deploy:webui": "nx generate-env-variables webui --skipNxCache && nx deploy webui --skipNxCache",
    "deploy:webui:production": "nx generate-env-variables webui --configuration production --skipNxCache && nx deploy webui --configuration production --skipNxCache",
    "test:backend": "nx run-many -t test -p tag:scope:backend tag:scope:backend-libs tag:scope:shared",
    "test:backend:init": "tsx ./scripts/backend/initLocalEnvironment.ts",
    "test:backend:ai-benchmark": "npx vitest run libs/backend/scripts/src/lib/AIServiceBenchmark/AIServiceBenchmark.test.ts",
    "lint:backend": "nx run-many -t lint -p tag:scope:backend tag:scope:backend-libs tag:scope:shared",
    "lint": "nx run-many -t lint --exclude di-container",
    "lint:fix": "nx run-many -t lint --fix --exclude di-container",
    "test": "nx run-many -t test",
    "format": "nx format:write --all",
    "format:check": "nx format:check --all",
    "generate-api-schema": "openapi-typescript ./swagger.yaml -o libs/shared/api-schema/src/lib/schema.d.ts --empty-objects-unknown",
    "typeorm": "dotenvx run -f .env.test -- tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d libs/backend/infrastructure/src/lib/config/typeorm/dataSource.ts",
    "typeorm:tenant": "dotenvx run -f .env.test -- tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d libs/backend/infrastructure/src/lib/config/typeorm/tenantDataSource.ts",
    "db:startSession": "tsx ./scripts/backend/dbStartSession.ts"
  },
  "nx": {
    "includedScripts": []
  },
